  "scripts": {
    "serve": "webpack serve --mode development",
    "dev:chrome": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=chrome webpack --watch",
    "dev:firefox": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=firefox webpack --watch",
    "dev:opera": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=opera webpack --watch",
    "build:chrome": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=chrome webpack",
    "build:firefox": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=firefox webpack",
    "build:opera": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=opera webpack",
    "build": "yarn run build:chrome && yarn run build:firefox && yarn run build:opera",
    "release:chrome": "cross-env NODE_ENV=production MODE=release cross-env TARGET_BROWSER=chrome webpack",
    "release:firefox": "cross-env NODE_ENV=production MODE=release cross-env TARGET_BROWSER=firefox webpack",
    "release:opera": "cross-env NODE_ENV=production MODE=release cross-env TARGET_BROWSER=opera webpack",
    "release": "yarn run build:chrome && yarn run build:firefox && yarn run build:opera",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "react-devtools": "react-devtools",
    "postinstall": "patch-package",
    "push-compiled": "cross-env ./push-compiled-extension.sh",
    "build-expose:chrome": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=chrome webpack && npm run push-compiled",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\""
  },
